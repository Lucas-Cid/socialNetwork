<%= button_to "Remover Foto", users_removeProfilePicture_path, :method => "get", style: "color: white;
	background-color: #444242;border:1px solid #444242;" %>
<%= form_with url: users_changeProfilePicture_path, :method => "post", authenticity_token: false do |f|%>
	<%= f.file_field :profilePicture %>
	<%= f.submit "Trocar foto", :class => 'btn btn-primay', data: { disable: true }, style: "color: white;
	background-color: #444242;"%>
<% end %>